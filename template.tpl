___SANDBOXED_JS_FOR_WEB_TEMPLATE___

// Geef hier uw templatecode op.
const log = require('logToConsole');
log('data =', data);

// Roep data.gtmOnSuccess aan wanneer de tag is voltooid.
data.gtmOnSuccess();

const injectScript = require('injectScript');
const encodeUriComponent = require('encodeUriComponent');
const logToConsole = require('logToConsole');

const token = data.token;
const loadJquery = data.load_jquery ? '1' : '0';

if (!token) {
  logToConsole('123 VCP Cookie Banner: geen token opgegeven');
  return;
}

const src =
  'https://www.visitpro.nl/cookie_manager.php' +
  '?token=' + encodeUriComponent(token) +
  '&load_jquery=' + loadJquery;

injectScript(
  src,
  function () {
    logToConsole('123 VCP Cookie Banner: CMP geladen');
  },
  function () {
    logToConsole('123 VCP Cookie Banner: laden mislukt');
  }
);


